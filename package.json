{
  "name": "member-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:ssg": "ng build --configuration production",
    "vercel-build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "seo:check": "node scripts/seo-check.js",
    "serve:prod": "npx http-server dist/member-client/browser -p 8080 -c-1",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --preset=mobile",
    "test:lighthouse": "npm run build:ssg && npm run serve:prod & sleep 5 && npm run lighthouse && kill %1",
    "test:seo": "npm run seo:check && npm run lighthouse",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/member-client/stats.json",
    "serve:ssr:member-client": "node dist/member-client/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.4",
    "@angular/cdk": "20.2.8",
    "@angular/common": "20.3.4",
    "@angular/compiler": "20.3.4",
    "@angular/core": "20.3.4",
    "@angular/forms": "20.3.4",
    "@angular/material": "20.2.8",
    "@angular/platform-browser": "20.3.4",
    "@angular/platform-browser-dynamic": "20.3.4",
    "@angular/platform-server": "20.3.4",
    "@angular/router": "20.3.4",
    "@angular/ssr": "20.3.5",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "20.3.5",
    "@angular/cli": "20.3.5",
    "@angular/compiler-cli": "20.3.4",
    "@lhci/cli": "^0.15.1",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
